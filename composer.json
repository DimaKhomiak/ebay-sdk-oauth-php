{
    "name": "dkh/ebay-sdk-oauth-php",
    "description": "An eBay SDK for PHP. Use the eBay API in your PHP projects.",
    "keywords": ["ebay", "api", "sdk"],
    "homepage": "https://github.com/DimaKhomiak",
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "Dmytro Khomiak",
            "email": "khomiak.dmytro@gmail.com",
            "homepage": "https://github.com/DimaKhomiak"
        }
    ],
    "support": {
        "issues": "https://github.com/DimaKhomiak/ebay-sdk-oauth-php"
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/psr7": "~1.0",
        "guzzlehttp/promises": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "DKH\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DKH\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}

